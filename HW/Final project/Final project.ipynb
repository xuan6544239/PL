    "    # 設置搜索半徑\n",
    "    radius = 1200  # 搜索半徑（以米為單位）\n",
    "        \n",
    "    # 使用Places API進行搜索\n",
    "    places = gmaps.places_nearby(location=location, keyword=keyword_input, radius=radius, language='zh-TW')\n",
    "    \n",
    "    # 計算店家數\n",
    "    number = 0\n",
    "    \n",
    "    # 列印店家資訊並儲存到 all_places 列表中\n",
    "    for place in places['results']:\n",

    "# 初始化情感分析結果的計數器\n",
    "positive_words = []\n",
    "negative_words = []\n",
    "neutral_words = []\n",
    "\n",
    "# 遍歷每個詞進行情感分析\n",
    "for word in seg_list:\n",
    "    # 使用 SnowNLP 進行情感分析\n",
    "    # 將字串轉換為 SnowNLP 實例\n",
    "    s = SnowNLP(word)\n",
    "\n",
    "    # 使用 SnowNLP 進行情感分析\n",
    "    sentiment_score = s.sentiments\n",
    "    # 根據情感概率判斷是正面還是負面，並更新對應列表\n",
    "    if sentiment_score > 0.5:\n",
    "        positive_words.append(word)\n",
    "    elif sentiment_score < 0.5:\n",
    "        negative_words.append(word)\n",
    "    else:\n",
    "    # 將情感得分為0.5視為中性詞彙\n",
    "        neutral_words.append(word)\n",
    "\n",
    "# 計算正面和負面詞彙的個數和比例\n",
    "total_positive_words = len(positive_words)\n",
    "total_negative_words = len(negative_words)\n",
    "total_neutral_words = len(neutral_words)\n",
    "total_words = total_positive_words + total_negative_words+total_neutral_words\n",
    "\n",
    "positive_ratio = total_positive_words / total_words\n",
    "negative_ratio = total_negative_words / total_words\n",
    "\n",
    "print(f\"總詞數: {total_words}\")\n",
    "print(f\"正面詞數: {total_positive_words}, 正面詞比例: {positive_ratio:.2%}\")\n",
    "print(f\"負面詞數: {total_negative_words}, 負面詞比例: {negative_ratio:.2%}\")\n"
